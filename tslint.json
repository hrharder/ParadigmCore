{
    "extends":"tslint:recommended",
    "rules":{
        "jsdoc-format": false,
        "variable-name": false,
        "no-angle-bracket-type-assertion": false,
        "no-var-requires":false,
        "trailing-comma":[false],
        "prefer-const":false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "interface-name": false,
        "one-variable-per-declaration": false
    }
}